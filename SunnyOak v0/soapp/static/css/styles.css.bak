 /* The switch - the box around the slider */
.switch-on-off {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}

.switch-on-off div {
  color: white;
  margin: 0rem 1rem;
  font-weight: bold;
}

.switch {
  position: relative;
  display: inline-block;
  width: 90px;
  height: 51px;
  margin-top: 25px;
}

/* Hide default HTML checkbox */
.switch input {
  opacity: 0;
  width: 0;
  height: 0;
}

 /* The switch - the box around the slider */
.switch-sequence {
  position: absolute;
  margin-left: 85%;
  width: 90px;
  height: 51px;
  margin-top: 10px;
}

/* Hide default HTML checkbox */
.switch-sequence input {
  opacity: 0;
  width: 0;
  height: 0;
}

/* The slider */
.slider {
  position: absolute;
  cursor: pointer;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background-color: #ccc;
  -webkit-transition: .4s;
  transition: .4s;
}

.slider:before {
  position: absolute;
  content: "";
  height: 39px;
  width: 39px;
  left: 6px;
  bottom: 6px;
  background-color: white;
  -webkit-transition: .4s;
  transition: .4s;
}

input:checked + .slider {
  background-color: #f1c40f;
}

input:focus + .slider {
  box-shadow: 0 0 1px #2196F3;
}

input:checked + .slider:before {
  -webkit-transform: translateX(39px);
  -ms-transform: translateX(39px);
  transform: translateX(39px);
}

/* Rounded sliders */
.slider.round {
  border-radius: 34px;
}

.slider.round:before {
  border-radius: 50%;
} 

.colorpicker{
  z-index: 1;
  width: 224px;
  background-color: #303030;
  outline : none;
}

input[type=range]{
    -webkit-appearance: none;
}

input[type=range]::-webkit-slider-thumb {
  outline : none;
  -webkit-appearance: none;
  height: 24px;
  width: 24px;
  border-radius: 50%;
  background: white;
  margin-top: -8px;
  cursor: pointer;   
}

.colorpicker::-moz-range-thumb{
  outline : none;
  border: none;
  width: 24px;
  height: 24px;
  box-shadow: 1px 1px 2px rgba(0, 0, 0, 0.2);
  border-radius: 100%;
  cursor: pointer;
}

.valider {
  position: relative;
  width: 18rem;
  margin: auto;
  text-align: center;
  display: flex;
  flex-direction: column;
}



.valider1{
  position: absolute;
  width: 65%;
