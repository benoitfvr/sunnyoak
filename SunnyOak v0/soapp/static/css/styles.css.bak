  border-radius: 20px;
  box-shadow: 6px 6px 6px black;
  z-index: 2;
}

.input {
  box-sizing: border-box;
  display: block;
  width: 100%; height: 3rem;
  appearance: none;
  font-family: 'Lobster Two', cursive;
  font-size: 1.2rem;
  color: #556;
  text-align: center;
  border: 0;
  border-radius: 1.5rem;
  background: #fff;
  text-shadow: 1px 1px 1px solid black;
}

.submit {
  top: 100px;
  margin-bottom: .6rem;
  font-weight: 500;
  cursor: pointer;
  color: hsl(0, 100%, 50%);
  background: #fff;
  transition: 200ms;
  font-weight: bold;
  text-shadow: 1px 1px 1px black;
  letter-spacing: 2px;
}

.submit:hover {
